

<div class="column main-pane">


  <div class= "content-pane-2">

<div class="row">
<div class="col-9" style="margin: 20px 10px 0px 40px">
<div class="title-text">Login</div>
<%= form_for @conn, Routes.session_path(@conn, :create), [as: :session, id: "login-form"], fn f -> %> 

<div class="form-group">
<%= text_input f, :matno, placeholder: "matno", class:
"form-control" %>
</div>
<div class="form-group">
<%= password_input f, :password, placeholder: "Password",
class: "form-control" %>
</div>
<%= submit "Log in", class: "btn btn-primary" %>
<% end %>
</div>
</div></div>
</div>